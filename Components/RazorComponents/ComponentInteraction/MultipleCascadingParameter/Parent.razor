<div>
    <h3>Parent Component</h3>
    <div>Received value1 at parent: @Data1</div>
    <div>Received value2 at parent: @ValueReceivedAtParent</div>
    <Child />

</div>

@code {
    [CascadingParameter]
    public string Data1 { get; set; } = "bbbbbbbbb";


    [CascadingParameter(Name = "CascadeParam2")]
    public string ValueReceivedAtParent { get; set; } = "aaaaaaaaaaaa";

}
