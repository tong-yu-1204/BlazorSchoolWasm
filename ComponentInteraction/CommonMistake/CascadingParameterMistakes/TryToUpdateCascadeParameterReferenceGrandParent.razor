@using Data.Models;
<div>
    <h3>Grand Parent Component</h3>
    The grand parent component is passing "@ExampleInstance.Data" down to its descendant components
    <button type="button" @onclick="StateHasChanged">Update UI</button>
    <CascadingValue Value="ExampleInstance">
        <TryToUpdateCascadeParameterReferenceParent />
    </CascadingValue>

</div>

@code {
    public ExampleClass ExampleInstance { get; set; } = new()
        {
            Data = "Fresh Data"
        };
}
